@model IEnumerable<ConsomiTounsiFront.Models.command>

@{
    ViewBag.Title = "Index";
}

<h2>Commands</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.reference)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.order_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.type)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.total_price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.chariot_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.client_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.delivery_id)
        </th>


        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.reference)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.order_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.total_price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.chariot.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.client.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.delivery.id)
            </td>


            <td>
                @Html.ActionLink("Edit", "Edit", new { reference = item.reference }) |
                @Html.ActionLink("Details", "Details", new { reference = item.reference }) |
                @Html.ActionLink("Delete", "Delete", new { reference = item.reference })
            </td>
        </tr>
    }

  


</table>
@{
    Layout = "~/Views/Shared/_CustomLayout.cshtml";
}